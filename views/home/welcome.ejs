<!DOCTYPE html>
<html>

<head>



  <!-- 3. opencv script loading -->


  <div class="caption">imageSrc <input type="file" id="fileInput" name="file" /></div>
  <img id="canvasInput" alt="No Image" width="400dp" height="360dp" />
  <p id="status">OpenCV.js is loading...</p>
  <button id='t'>"z"</button>
  <br>

</head>

<body>
  <div class="container mb-3">
    <table>
      <% imagelist.forEach(function (element, count = 0) {
      %>
      <tr>
        <td>
          <img src="./res/<%=element%>" id="<%- count %>" class="input" />
        </td>
        <!-- 'tophat_temp', 'tophat_most', -->
        <% var cvlist = ['gray', 'gray2']%>
        <% cvlist.forEach(function(element2) {%>
        <td>
          <canvas id="o<%= count %><%=element2%>" class="<%=element2%>" />
        </td>
        <% })%>

        </br>
      </tr>
      <% count++;})%>
    </table>


  </div>


  <script src='./js/opcv_js/convert.js' type="text/javascript"> </script>
  <script async src="./js/opencv.js" onload="onOpenCvReady();" type="text/javascript">
  </script>

</body>


</html>